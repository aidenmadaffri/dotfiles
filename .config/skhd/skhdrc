# Display and space management
ctrl + alt - escape : yabai -m display --focus recent

ctrl + alt - 1 : yabai -m space --focus A1
ctrl + alt - 2 : yabai -m space --focus A2
ctrl + alt - 3 : yabai -m space --focus A3
ctrl + alt - 4 : yabai -m space --focus A4
ctrl + alt - 5 : yabai -m space --focus A5
rcmd + ctrl + alt - 1 : yabai -m space --focus B1
rcmd + ctrl + alt - 2 : yabai -m space --focus B2
rcmd + ctrl + alt - 3 : yabai -m space --focus B3
rcmd + ctrl + alt - 4 : yabai -m space --focus B4
rcmd + ctrl + alt - 5 : yabai -m space --focus B5

# Window management
ctrl + alt - up : yabai -m window --focus north
ctrl + alt - down : yabai -m window --focus south
ctrl + alt - left : yabai -m window --focus west
ctrl + alt - right : yabai -m window --focus east

shift + ctrl + alt - up : yabai -m window --swap north
shift + ctrl + alt - down : yabai -m window --swap south
shift + ctrl + alt - left : yabai -m window --swap west
shift + ctrl + alt - right : yabai -m window --swap east

shift + ctrl + alt - 1 : yabai -m window --space A1 --follow; yabai -m space --focus A1
shift + ctrl + alt - 2 : yabai -m window --space A2 --follow; yabai -m space --focus A2
shift + ctrl + alt - 3 : yabai -m window --space A3 --follow; yabai -m space --focus A3
shift + ctrl + alt - 4 : yabai -m window --space A4 --follow; yabai -m space --focus A4
shift + ctrl + alt - 5 : yabai -m window --space A5 --follow; yabai -m space --focus A5

rcmd + shift + ctrl + alt - 1 : yabai -m window --space B1 --follow; yabai -m space --focus B1
rcmd + shift + ctrl + alt - 2 : yabai -m window --space B2 --follow; yabai -m space --focus B2
rcmd + shift + ctrl + alt - 3 : yabai -m window --space B3 --follow; yabai -m space --focus B3
rcmd + shift + ctrl + alt - 4 : yabai -m window --space B4 --follow; yabai -m space --focus B4
rcmd + shift + ctrl + alt - 5 : yabai -m window --space B5 --follow; yabai -m space --focus B5

# Full Screen
ctrl + alt - f : yabai -m window --toggle zoom-fullscreen
# Close window
ctrl + alt - q : yabai -m window --close
# Toggle floating
shift + ctrl + alt - return : yabai -m window --toggle float

# Launch applications
ctrl + alt - x : osascript -e "tell application \"iTerm2\" to set newWindow to (create window with default profile)" # iTerm2
shift + ctrl + alt - x : /Users/aiden/.scripts/emacs-starter.pl
shift + ctrl + alt - p : open -n /Applications/Plex.app
shift + ctrl + alt - a : open -n /Applications/Anki.app
ctrl + alt - d : open ~ # Finder
ctrl + alt - b : open -n /Applications/Google\ Chrome.app

# Sceenshots
f13 : /Users/aiden/.scripts/screenshot.sh
shift - f13 : /Users/aiden/.scripts/screenshotpath.sh

# Sleep screen
ctrl + alt + shift - d : pmset displaysleepnow
