{
  "title": "Emacs Changes",
  "rules": [
    {
      "description": "Swap Command with Ctrl in Emacs",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^org\\.gnu\\.Emacs$",
                "^org\\.gnu\\.AquamacsEmacs$",
                "^org\\.gnu\\.Aquamacs$",
                "^org\\.pqrs\\.unknownapp.conkeror$"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "left_shift",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_shift",
              "lazy": true
            }
          ],
          "conditions": [
              {
                "type": "frontmost_application_if",
                "bundle_identifiers": [
                  "^org\\.gnu\\.Emacs$",
                  "^org\\.gnu\\.AquamacsEmacs$",
                  "^org\\.gnu\\.Aquamacs$",
                  "^org\\.pqrs\\.unknownapp.conkeror$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
